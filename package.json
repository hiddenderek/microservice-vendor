{
    "name": "microservice-orders",
    "version": "0.0.1",
    "description": "order app",
    "main": "index.js",
    "scripts": {
        "dev": "tsup src/index.ts --watch --onSuccess 'node dist/index.js'",
        "format": "npm run prettier -- --write --list-different",
        "prettier": "prettier . --ignore-unknown",
        "build": "tsup src/index.ts",
        "build-ci": "npm ci && npm run build && npm prune --production",
        "test": "jest --runInBand -c jest.config.js",
        "test:integration": "NODE_ENV=migrate DB_USERID=postgres DB_PASSWORD=Secret1234 DB_PORT=3307 DB_NAME=backenddb db-migrate up && NODE_ENV=development jest --runInBand -c jest.config.integration.js --detectOpenHandles",
        "start": "node dist/index.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@nestjs/axios": "^3.0.3",
        "@nestjs/common": "^10.3.3",
        "@nestjs/core": "^10.3.3",
        "@nestjs/swagger": "^7.3.0",
        "@nestjs/testing": "^10.4.1",
        "axios": "^1.7.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "fix-esm": "^1.0.1",
        "pg": "^8.12.0",
        "pg-template-tag": "^0.1.2",
        "sql-tag": "^1.0.1",
        "sql-template-tag": "^5.2.1",
        "typescript": "^5.5.4",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@swc/core": "^1.4.2",
        "@types/jest": "^29.5.12",
        "@types/pg": "^8.11.6",
        "db-migrate": "^0.11.14",
        "db-migrate-pg": "^1.5.2",
        "esm": "^3.2.25",
        "jest": "^29.7.0",
        "jwt-simple": "^0.5.6",
        "prettier": "^3.3.3",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2"
    }
}
